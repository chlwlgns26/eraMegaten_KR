@IS_GAY,ARG = -1,ARG:1
#FUNCTION
SIF ARG == -1
	ARG = TARGET
	
SELECTCASE FLAG:男の娘ＢＬ設定
	CASE 0
		RETURNF IS_MALE(ARG) && !TALENT:ARG:男の娘 && IS_MALE(ARG:1) && !TALENT:(ARG:1):男の娘
	CASE 1
		RETURNF IS_MALE(ARG) && IS_MALE(ARG:1)
	CASE 2
		RETURNF IS_MALE(ARG) && IS_MALE(ARG:1) && (!TALENT:ARG:男の娘 || !TALENT:(ARG:1):男の娘)
ENDSELECT